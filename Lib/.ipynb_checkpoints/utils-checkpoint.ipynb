    "def mkdirs(folder, permission):\n",
    "       if not os.path.exists(folder):\n",
    "           try:\n",
    "                 original_umask = os.umask(0)\n",
    "                 os.makedirs(folder, permission, exist_ok=True)\n",
    "           finally:\n",
    "                  os.umask(original_umask)"
   ]
  }
